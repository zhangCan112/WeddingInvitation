<!--vendor/@xsy-customize/prm-component-welcome/welcome/welcome.wxml-->
<swiper class="swiper-default swiper-class" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imageUrls}}" wx:key="index">
    <swiper-item>
      <view class="swiper-default swiper-class" style="background-image: url({{item}});background-size:100vw 100vh;">
        <view wx:if="{{index == imageUrls.length-1}}">
          <slot></slot>
        </view>
      </view>
    </swiper-item>
  </block>
</swiper>
